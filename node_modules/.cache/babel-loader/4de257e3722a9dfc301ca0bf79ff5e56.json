sion.resource },\n      );\n    },\n    removePermission(id) {\n      this.policy.permissions = this.policy.permissions\n        .filter((perm) => perm.id !== id);\n    },\n  },\n};\n</script>\n\n<style>\nul {\n  list-style-type: none;\n  margin: 10px;\n  padding: 10px;\n}\n\n/*#newPolicy {\n  display: inline-flex;\n}*/\n\ninput {\n  width:80px;\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}