AAA,MAAA,IAAA,CAAA;AACA,WAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CACA;AAAA,QAAA,EAAA,EAAA,MAAA;AAAA,QAAA,MAAA,EAAA,KAAA,UAAA,CAAA,MAAA;AAAA,QAAA,QAAA,EAAA,KAAA,UAAA,CAAA;AAAA,OADA;AAGA,KAhBA;AAiBA,IAAA,gBAjBA,4BAiBA,EAjBA,EAiBA;AACA,WAAA,MAAA,CAAA,WAAA,GAAA,KAAA,MAAA,CAAA,WAAA,CACA,MADA,CACA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OADA,CAAA;AAEA;AApBA;AAvBA,CAAA","sourcesContent":["<template>\n<div id=\"newPolicy\">\n    <div id=\"policy-effect\">\n        <span>Effect:</span>\n        <select v-model=\"policy.effect\">\n            <option value=\"grant\">GRANT</option>\n            <option value=\"deny\">DENY</option>\n        </select>\n    </div>\n    <div id=\"policy-principals\">\n        <span>Principals:</span>\n        <select v-model=\"principal.type\">\n            <option disabled value=\"\">principal type</option>\n            <option value=\"user\">USER</option>\n            <option value=\"group\">GROUP</option>\n            <option value=\"role\">ROLE</option>\n            <option value=\"entity\">ENTITY</option>\n        </select>\n        <input v-model=\"principal.name\" placeholder=\"principal name\">\n        <button @click=\"addPrincipal\">=></button>\n        <ul v-if=\"policy.principals.length\">\n          <Principal\n            v-for=\"p in policy.principals\"\n            :key=\"p.id\"\n            :principal=\"p\"\n            @removePrincipal=\"removePrincipal\"\n            />\n        </ul>\n    </div>\n    <div id=\"policy-permissions\">\n        <span>Permissions:</span>\n        <input v-model=\"permission.action\" placeholder=\"action\">\n        <input v-model=\"permission.resource\" placeholder=\"resource\">\n        <button @click=\"addPermission\">=></button>\n        <ul v-if=\"policy.permissions.length\">\n          <Permission\n            v-for=\"perm in policy.permissions\"\n            :key=\"perm.id\"\n            :permission=\"perm\"\n            @removePermission=\"removePermission\"\n            />\n        </ul>\n    </div>\n    <div id=\"policy-condition\">\n        <span>Condition:</span>\n        <input v-model=\"policy.condition\" placeholder=\"Pls enter condition if any\">\n    </div>\n    <button @click=\"$emit('addPolicy', policy)\">\n      Create\n    </button>\n</div>\n</template>\n\n<script>\nimport Principal from './Principal.vue';\nimport Permission from './Permission.vue';\n\nlet prinID = 0;\nlet permID = 0;\n\nexport default {\n  name: 'NewPolicy',\n  components: {\n    Principal, Permission,\n  },\n  data() {\n    return {\n      policy: {\n        effect: '',\n        principals: [{ type: 'user', name: 'aa' }],\n        permissions: [],\n        condition: '',\n      },\n      principal: {\n        type: '',\n        name: '',\n      },\n      permission: {\n        action: '',\n        resource: '',\n      },\n    };\n  },\n  methods: {\n    addPrincipal() {\n      prinID += 1;\n      this.policy.principals.push(\n        { id: prinID, type: this.principal.type, name: this.principal.name },\n      );\n    },\n    removePrincipal(id) {\n      this.policy.principals = this.policy.principals\n        .filter((principal) => principal.id !== id);\n    },\n    addPermission() {\n      permID += 1;\n      this.policy.permissions.push(\n        { id: permID, action: this.permission.action, resource: this.permission.resource },\n      );\n    },\n    removePermission(id) {\n      this.policy.permissions = this.policy.permissions\n        .filter((perm) => perm.id !== id);\n    },\n  },\n};\n</script>\n\n<style>\nul {\n  list-style-type: none;\n  margin: 10px;\n  padding: 10px;\n}\n\n/*#newPolicy {\n  display: inline-flex;\n}*/\n\ninput {\n  width:80px;\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}