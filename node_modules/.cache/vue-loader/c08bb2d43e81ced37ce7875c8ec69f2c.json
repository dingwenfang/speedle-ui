-col cols=\"5\" align-self=\"center\">\n            <b-row>\n              <b-col cols=\"5\" class=\"pr-0\" >\n                <b-form-input v-model=\"permission.action\" placeholder=\"action\"></b-form-input>\n              </b-col>\n              <b-col cols=\"7\" >\n                <b-form-input v-model=\"permission.resource\" placeholder=\"resource\"></b-form-input>\n              </b-col>\n            </b-row>\n          </b-col>\n        </b-row>\n      </b-form-group>\n      <b-form-group\n        label-cols-sm=\"2\"\n        label=\"Condition:\"\n        label-align-sm=\"right\"\n        label-for=\"policy-condition\"\n      >\n        <b-row >\n          <b-col cols=\"5\">\n            <b-form-input id=\"policy-condition\"></b-form-input>\n          </b-col>\n        </b-row>\n      </b-form-group>\n    </b-form-group>\n    <b-button variant=\"outline-success\" @click=\"$emit('addPolicy', policy)\">\n      Create\n    </b-button>\n  </b-card>\n</template>\n\n<script>\nimport Principal from './Principal.vue';\nimport Permission from './Permission.vue';\n\nlet prinID = 0;\nlet permID = 0;\n\nexport default {\n  name: 'NewPolicy',\n  components: {\n    Principal, Permission,\n  },\n  data() {\n    return {\n      policy: {\n        effect: '',\n        principals: [{ type: 'user', name: 'aa' }],\n        permissions: [],\n        condition: '',\n      },\n      effectOptions: [\n        { value: 'grant', text: 'GRANT' },\n        { value: 'deny', text: 'DENY' },\n      ],\n      principalTypeOptions: [\n        { value: 'user', text: 'User' },\n        { value: 'group', text: 'Group' },\n        { value: 'role', text: 'Role' },\n        { value: 'entity', text: 'Entity' },\n      ],\n      principal: {\n        type: '',\n        name: '',\n      },\n      permission: {\n        action: '',\n        resource: '',\n      },\n    };\n  },\n  methods: {\n    addPrincipal() {\n      prinID += 1;\n      this.policy.principals.push(\n        { id: prinID, type: this.principal.type, name: this.principal.name },\n      );\n    },\n    removePrincipal(id) {\n      this.policy.principals = this.policy.principals\n        .filter((principal) => principal.id !== id);\n    },\n    addPermission() {\n      permID += 1;\n      this.policy.permissions.push(\n        { id: permID, action: this.permission.action, resource: this.permission.resource },\n      );\n    },\n    removePermission(id) {\n      this.policy.permissions = this.policy.permissions\n        .filter((perm) => perm.id !== id);\n    },\n  },\n};\n</script>\n\n<style>\nul {\n  list-style-type: none;\n  margin: 10px;\n  padding: 10px;\n}\n\n/*#newPolicy {\n  display: inline-flex;\n}*/\n\ninput {\n  width:80px;\n}\n\n</style>\n"]}]}